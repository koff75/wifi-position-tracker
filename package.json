  },
  "homepage": "https://github.com/koff75/wifi-position-tracker#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "iconv-lite": "^0.7.0",
    "inquirer": "^12.9.6",
    "p-limit": "^7.1.1",
    "protobufjs": "^7.5.4",
