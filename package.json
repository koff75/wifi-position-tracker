{
  "name": "wifi-position-tracker",
  "version": "1.0.0",
  "description": "A Node.js toolkit for geolocating WiFi networks (BSSIDs) using Apple's undocumented location services API. This project contains two main tools:",
  "main": "bssid-geolocator.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate-proto": "pbjs -t static-module -w commonjs -o bssid_pb.js bssid.proto && pbts -o bssid_pb.d.ts bssid_pb.js",
    "web:install": "cd web && npm install",
    "web:build": "cd web && npm run build",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koff75/wifi-position-tracker.git"
  },
